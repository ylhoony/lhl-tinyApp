<!DOCTYPE html>
<html lang="en">
<head>
  <title>TinyApp</title>
  <link rel="stylesheet" href="./style/style.css" type="text/css">
</head>
<body>
  <!-- Add EJS code -->
  <header>
    <% include ./partials/_header %>
  </header>
      <p><a href="/urls/new">Create a New Short Link</a></p>
        <% for (let prop in userData) { %>
          <table class="list">
            <tr>
              <td>
                <a href="/u/<%=[prop]%>" target="_blank"><%=prop%></a>
              </td>

              <td>
                <p><%=userData[prop]%></p>
              </td>

              <td>
                <p><a href="/urls/<%=prop%>"">Edit</a></p>
              </td>

              <td>
                <form method="POST" action="/urls/<%=prop%>/delete">
                  <button id="Btn">Delete</button>
                </form>
              </td>
            </tr>
          <table>
        <% } %>
</body>
</html>